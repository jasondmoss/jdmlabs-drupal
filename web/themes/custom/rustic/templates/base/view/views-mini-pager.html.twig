{% if items.previous or items.next -%}
  <nav itemscope itemtype="https://schema.org/pagination" aria-labelledby="paginationLabel">
    <h2 id="paginationLabel" class="sr-only">{{ 'Pagination'|t }}</h2>
    <ul class="js-pager__items pagination">
      {% if items.previous -%}
        <li>
          <a rel="prev" href="{{ items.previous.href }}" title="{{ 'Go to previous page'|t }}"{{ items.previous.attributes|without('href', 'title', 'rel') }}>
            <span class="sr-only">{{ 'Previous page'|t }}</span>
            <span aria-hidden="true">{{ items.previous.text|default('‹‹'|t) }}</span>
          </a>
        </li>
      {% endif -%}
      {% if items.current -%}
        <li><span>{% trans %}Page {{ items.current }}{% endtrans %}</span></li>
      {% endif -%}
      {% if items.next -%}
        <li>
          <a rel="next" href="{{ items.next.href }}" title="{{ 'Go to next page'|t }}"{{ items.next.attributes|without('href', 'title', 'rel') }}>
            <span class="sr-only">{{ 'Next page'|t }}</span>
            <span aria-hidden="true">{{ items.next.text|default('››'|t) }}</span>
          </a>
        </li>
      {% endif -%}
    </ul>
  </nav>
{% endif -%}
